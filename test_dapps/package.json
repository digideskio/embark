{
  "devDependencies": {
    "cross-env": "5.2.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3"
  },
  "engines": {
    "node": ">=8.12.0",
    "npm": ">=6.4.1",
    "yarn": ">=1.12.3"
  },
  "name": "test_dapps",
  "private": true,
  "scripts": {
    "clean": "npx rimraf **/.embark **/chains.json **/coverage **/dist **/node_modules/.cache",
    "install:embark": "node scripts/install-embark",
    "test": "cross-env TEST=test TEST_DAPP=test_dapp yarn test_",
    "test:test_app": "cross-env DAPP=test_app yarn test__",
    "test:contracts_app": "cross-env DAPP=contracts_app yarn test__",
    "pretestQA": "yarn install:embark",
    "reset": "npx rimraf **/node_modules .embark node_modules",
    "testQA": "cross-env TEST=testQA TEST_DAPP=test_dappQA yarn test_",
    "test_": "npm-run-all test:*",
    "test__": "cross-env-shell \"yarn $TEST_DAPP\"",
    "test_dapp": "cross-env-shell \"cd $DAPP && npm install && yarn $TEST\"",
    "test_dappQA": "cross-env EMBARK_NO_SHIM=t yarn test_dapp"
  }
}
