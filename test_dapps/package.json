{
  "devDependencies": {
    "cross-env": "5.2.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "lerna": "3.10.5",
    "rimraf": "2.6.3"
  },
  "engines": {
    "node": ">=8.12.0",
    "npm": ">=6.4.1",
    "yarn": ">=1.12.3"
  },
  "name": "test_dapps",
  "private": true,
  "scripts": {
    "CI": "npm run QA",
    "QA": "npm run install:embark",
    "boot": "yarn install",
    "clean": "npx cross-env CMD=clean_ npm run with-embark",
    "clean_": "npm run lrunp clean",
    "cwtree": "node ../scripts/check-working-tree",
    "install:embark": "node scripts/install-embark",
    "lerna": "npx lerna",
    "lexec": "npm run lerna exec",
    "lexecp": "npm run lexec -- --parallel",
    "lexecs": "npm run lexec -- --stream",
    "lrun": "npm run lerna run",
    "lrunp": "npm run lrun -- --parallel",
    "lruns": "npm run lrun -- --stream",
    "postinstall": "npm run lerna bootstrap",
    "postreboot": "npm run boot",
    "postreboot_": "npm run postreboot",
    "postreset_": "npx rimraf .embark node_modules",
    "posttestQA_": "npm run cwtree",
    "preclean": "npx rimraf packages/embark_demo",
    "prereset": "npm run clean",
    "pretestCI": "npm run boot && npm run CI",
    "pretestQA": "npm run reboot_ && npm run QA",
    "pretest_": "npm run setup:demo",
    "reboot": "npm run reset",
    "reboot_": "npm run reset_",
    "reset": "npm run lrunp reset",
    "reset_": "npm run reset",
    "setup:demo": "node scripts/setup-demo",
    "test": "cross-env CMD=test_ npm run with-embark",
    "testCI": "npm run testQA_",
    "testQA": "npm run testQA_",
    "testQA_": "cross-env CMD=test_ npm run with-embark:pkg",
    "test_": "npm run lruns -- --concurrency=1 test",
    "with-embark": "npx cross-env EMBARK_BIN=../../../packages/embark/bin/embark SCRIPT_EMBARK_BIN=../../packages/embark/bin/embark npm run with-embark_",
    "with-embark:pkg": "npx cross-env EMBARK_BIN=../../.embark/packaged/node_modules/embark/bin/embark SCRIPT_EMBARK_BIN=../.embark/packaged/node_modules/embark/bin/embark npm run with-embark_",
    "with-embark_": "cross-env-shell EMBARK_NO_SHIM=t \"npm run $CMD\""
  }
}
