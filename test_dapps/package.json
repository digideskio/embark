{
  "devDependencies": {
    "cross-env": "5.2.0",
    "fs-extra": "7.0.1",
    "glob": "7.1.3",
    "lerna": "3.10.5",
    "rimraf": "2.6.3"
  },
  "engines": {
    "node": ">=8.12.0",
    "npm": ">=6.4.1",
    "yarn": ">=1.12.3"
  },
  "name": "test_dapps",
  "private": true,
  "scripts": {
    "CI": "yarn QA",
    "QA": "yarn install:embark",
    "boot": "yarn install",
    "clean": "npx cross-env CMD=clean_ yarn with-embark",
    "clean_": "yarn lrunp clean",
    "cwtree": "node ../scripts/check-working-tree",
    "install:embark": "node scripts/install-embark",
    "lerna": "npx lerna",
    "lexec": "yarn lerna exec",
    "lexecp": "yarn lexec --parallel",
    "lexecs": "yarn lexec --stream",
    "lrun": "yarn lerna run",
    "lrunp": "yarn lrun --parallel",
    "lruns": "yarn lrun --stream",
    "postinstall": "yarn lerna bootstrap",
    "postreboot": "yarn boot",
    "postreboot_": "yarn postreboot",
    "postreset_": "npx rimraf .embark node_modules",
    "posttestQA_": "yarn cwtree",
    "preclean": "npx rimraf packages/embark_demo",
    "prereset": "yarn clean",
    "pretestCI": "yarn boot && yarn CI",
    "pretestQA": "yarn reboot_ && yarn QA",
    "pretest_": "yarn setup:demo",
    "reboot": "yarn reset",
    "reboot_": "yarn reset_",
    "reset": "yarn lrunp reset",
    "reset_": "yarn reset",
    "setup:demo": "node scripts/setup-demo",
    "test": "cross-env CMD=test_ yarn with-embark",
    "testCI": "yarn testQA_",
    "testQA": "yarn testQA_",
    "testQA_": "cross-env CMD=test_ yarn with-embark:pkg",
    "test_": "yarn lruns --concurrency=1 test",
    "with-embark": "npx cross-env EMBARK_BIN=../../../packages/embark/bin/embark SCRIPT_EMBARK_BIN=../../packages/embark/bin/embark yarn with-embark_ yarn with-embark_",
    "with-embark:pkg": "npx cross-env EMBARK_BIN=../../.embark/packaged/node_modules/embark/bin/embark SCRIPT_EMBARK_BIN=../.embark/packaged/node_modules/embark/bin/embark yarn with-embark_ yarn with-embark_",
    "with-embark_": "cross-env-shell EMBARK_NO_SHIM=t \"yarn $CMD\""
  }
}
